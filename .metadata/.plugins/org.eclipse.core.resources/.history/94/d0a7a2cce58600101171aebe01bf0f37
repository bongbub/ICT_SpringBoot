package com.boot.springBoot_basic.dao;

import java.util.List;

import com.boot.springBoot_basic.dto.ProductDTO_no_lombok;

public interface ProductDAO {
	
	// list
	public List<ProductDTO_no_lombok> productList();
	
	
	// insert
	public int insertProduct(ProductDTO_no_lombok dto);
	
	
	// update
	public int updateProduct(ProductDTO_no_lombok dto);
	
	
	// delete
	public int updateProduct(int id);
	
	
	public ProductDTO_no_lombok findById(int id);
	
	

}
